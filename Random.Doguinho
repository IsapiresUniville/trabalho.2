.css

html, body {
    height: 100%;
    margin: 0;
    padding: 0;
  }
.fundo{
    width: 100%;
    height: 100%;
    background-color: #f1f1f1;
}

.borda{
  position: absolute;
  top: 0px;
  z-index: 1;
  width: 100%;
  height: 80px;
  background-color: #FFDF2B;
}

.logo{
  color: #34266B;
  position: absolute;
  z-index: 1;
  top: 3px;
  line-height: 0;
  left: 18px;
  text-align: center;
} 

.r{
  font-family: 'Chewy', cursive;
  font-size: 30px;
}

.d{
  font-family: 'League Spartan', sans-serif;
  font-size: 10px;
  font-weight: bold;
}

.frases{
  position: absolute;
  z-index: 1;
  top: 60px;
}

.t{
  position: fixed;
  top: 22%;
  left: 50%;
  transform: translate(-50%, -50%);
  margin-top: 10px;
  font-family: 'Lexend Peta', sans-serif;
  font-size: 29px;
  color: #34266B;
  font-weight: bold;
}

.a{
  position: fixed;
  top: 31%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-family: 'Merriweather', serif;
  font-size: 16px;
  color: #000000;
  font-weight: bold;
  text-align: center;
}

.i{
  position: absolute;
  z-index: 1;
  top: 100px;
  left: 60px;
}

.m{
  position: absolute;
  z-index: 1;
  top: 350px;
  left: 90px;
}

.g{
  position: absolute;
  z-index: 2;
  top: 100px;
  right: 90px;
}

.e{
  position: absolute;
  z-index: 2;
  top: 360px;
  right: 70px;
}

.body{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100vh;
}

.botao {
  display: inline-block;
  padding: 10px 20px;
  background-color: #FFDF2B;
  border: none;
  border-radius: 20px;
  text-align: center;
  text-decoration: none;
  color: #333;
  font-family: 'Lato', sans-serif;
  cursor: pointer;
  position: absolute;
  z-index: 1;
  top: 590px;
  font-weight: bold;
  font-size: 10px;
  color: #827219;
}


